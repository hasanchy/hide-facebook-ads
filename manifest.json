{
  "manifest_version": 3,
  "name": "Hide Facebook Sponsored Posts",
  "version": "1.0",
  "description": "Hides sponsored posts on Facebook",
  "permissions": [
    "activeTab",
    "webNavigation"
  ],
  "content_scripts": [
    {
      "matches": ["*://www.facebook.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
